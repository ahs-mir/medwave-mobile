{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": false
      },
      "ios": {
        "resourceClass": "large"
      }
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://rwave-staging.up.railway.app/api"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "cache": {
        "disabled": false
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://slippery-glass-production.up.railway.app/api",
        "DEV_QUICK_LOGIN_ENABLED": "false"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "large"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleTeamId": "B87HM5RH7R",
        "ascAppId": "6751819516"
      }
    }
  }
}
